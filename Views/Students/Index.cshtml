@model IEnumerable<UniversityApp.Models.Student>


<h2>Студенты</h2>
<table class="=table">
    <thead>
        <tr>
            <th>имя</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Редоктировать</a> |
                     <a asp-action="Delete" asp-route-id="@item.Id">Удалить</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create" class="btn btn-primary">Добавить студента</a>